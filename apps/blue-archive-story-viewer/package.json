{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build:json": "node ./CICD/build_json.mjs",
    "write:version": "node ./CICD/write_version.mjs",
    "build": "pnpm build:json && vite build && pnpm write:version && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build:json && vite preview --host",
    "check": "eslint --fix --config ./.eslintrc.json ./src/ ./CICD/ ./public/config/yaml/ --ext .js,.ts,.vue,.yml,.yaml && vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.3.4",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.1",
    "ba-story-player": "workspace:*",
    "fastest-levenshtein": "^1.0.16",
    "gsap": "^3.12.2",
    "husky": "^8.0.3",
    "ismobilejs": "^1.1.1",
    "mitt": "^3.0.0",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0",
    "pinyin-pro": "^3.15.4",
    "toastify-js": "^1.12.0",
    "typed.js": "^2.0.16",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "xxhashjs": "^0.2.2",
    "tailwindcss": "~3.3.3",
    "autoprefixer": "~10.4.14",
    "dayjs": "~1.11.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@csstools/css-parser-algorithms": "^2.2.0",
    "@csstools/css-tokenizer": "^2.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.3.3",
    "@types/toastify-js": "^1.11.1",
    "@types/xxhashjs": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-legacy": "^4.0.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "chalk": "^5.3.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-vue": "^9.15.1",

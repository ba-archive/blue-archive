    "js-yaml": "^4.1.0",
    "postcss": "^8.4.24",
    "postcss-plugin-px2rem": "^0.8.1",
    "postcss-preset-env": "^8.5.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "terser": "^5.18.2",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.16.4",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.3",
    "workbox-window": "^7.0.0",
    "ba-rig": "workspace:*"

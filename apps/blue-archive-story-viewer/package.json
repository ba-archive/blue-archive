{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --open",
    "build:json": "node ./CICD/build_json.mjs",
    "build": "pnpm build:json && vite build && cp ./dist/index.html ./dist/404.html",
    "build:github": "pnpm build:json && vite build --base=/blue-archive/ && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build:json && vite preview --host",
    "check": "eslint --fix ./src/ ./CICD/ ./public/config/yaml/ --ext .js,.ts,.vue,.yml,.yaml && vue-tsc --noEmit --skipLibCheck",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.4.27",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "axios-retry": "^4.5.0",
    "ba-story-player": "workspace:*",
    "fastest-levenshtein": "^1.0.16",
    "gsap": "^3.12.5",
    "husky": "^9.0.11",
    "ismobilejs": "^1.1.1",
    "mitt": "^3.0.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "pinyin-pro": "^3.20.4",
    "typed.js": "^2.1.0",
    "vue": "^3.5.12",
    "vue-router": "~4.3.0",
    "xxhashjs": "^0.2.2",
    "tailwindcss": "~3.4.1",
    "autoprefixer": "~10.4.17",
    "dayjs": "~1.11.11",
    "element-plus": "^2.8.6",
    "radash": "~12.1.0",
    "vite-plugin-vue-devtools": "^7.5.4",
    "unocss": "^0.59.0-beta.1"
  },
  "devDependencies": {
    "taze": "^0.17.2",
    "@babel/preset-env": "^7.24.5",
    "@csstools/css-parser-algorithms": "^2.6.3",
    "@csstools/css-tokenizer": "^2.3.1",
    "@types/node": "^22.8.0",
    "@types/toastify-js": "^1.12.3",
    "@types/xxhashjs": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-legacy": "^5.4.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "chalk": "^5.3.0",
    "eslint": "9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-vue": "^9.29.1",
    "eslint-plugin-yaml": "^1.0.3",
    "js-yaml": "^4.1.0",
    "postcss": "~8.4.44",
    "postcss-plugin-px2rem": "^0.8.1",
    "postcss-preset-env": "^9.5.13",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.80.4",
    "terser": "^5.31.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.20.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6",
    "workbox-window": "^7.1.0",
    "ba-rig": "workspace:*"
  }
}

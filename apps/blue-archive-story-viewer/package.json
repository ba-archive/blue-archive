{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "format": "prettier --config .prettierrc.json -uw ./src/ ./CICD/ ./public/**/*.yaml ./public/**/*.yml *.md",
    "build:json": "eslint --fix --config ./.eslintrc.json ./public/config/yaml/ && node ./CICD/build_json.js",
    "build": "pnpm format; pnpm build:json && vue-tsc --noEmit --skipLibCheck && vite build",
    "build:github": "pnpm build; cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build:json && vite build; vite preview --host",
    "check": "eslint --fix --config ./.eslintrc.json ./src/ ./CICD/ ./public/config/yaml/ --ext .js,.ts,.vue,.yml,.yaml && vue-tsc --noEmit --skipLibCheck",
    "prepare": "husky install"
  },
  "lint-staged": {
    "public/**/*.{yaml, yml}": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ],
    "src/**/*.{ts,js,vue}": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ],
    "CICD/**/*.{ts,js}": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ],
    "*.md": [
      "prettier --config .prettierrc.json --write",
      "git add"
    ]
  },
  "dependencies": {
    "@vue/compiler-sfc": "3.x",
    "@vueuse/core": "^10.0.0",
    "axios": "1.4.0",
    "axios-retry": "3.5.0",
    "ba-story-player": "workspace:^1.0.0",
    "fastest-levenshtein": "1.0.16",
    "gsap": "^3.11.3",
    "husky": "^8.0.3",
    "ismobilejs": "^1.1.1",
    "mitt": "^3.0.0",
    "pinia": "2.1.3",
    "pinia-plugin-persistedstate": "3.1.0",
    "toastify-js": "1.12.0",
    "typed.js": "^2.0.12",
    "vue": "3.3.4",
    "vue-router": "4.2.1",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@csstools/css-parser-algorithms": "^2.1.0",
    "@csstools/css-tokenizer": "^2.1.0",
    "@rushstack/eslint-patch": "1.3.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "^18.11.9",
    "@types/toastify-js": "1.11.1",
    "@types/xxhashjs": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "@vitejs/plugin-legacy": "4.0.4",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/eslint-config-typescript": "11.0.3",
    "chalk": "5.2.0",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.14.1",
    "eslint-plugin-yaml": "0.5.0",
    "js-yaml": "4.1.0",
    "postcss": "8.4.24",
    "postcss-plugin-px2rem": "0.8.1",
    "postcss-preset-env": "8.4.1",
    "prettier": "2.8.8",
    "prettier-plugin-css-order": "1.3.0",
    "rollup-plugin-visualizer": "5.9.0",
    "sass": "1.62.1",
    "terser": "5.17.6",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-plugin-clear-console": "^0.2.2",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.15.1",
    "vue-tsc": "1.6.5",
    "workbox-window": "7.0.0"
  }
}

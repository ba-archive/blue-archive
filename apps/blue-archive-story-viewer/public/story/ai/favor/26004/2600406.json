    },
    {
      "TextJp": "[ns5]  \"……それに？\"\n[ns6]  \"……シミコ？\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2600406,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns5] \"……게다가?\"\n[ns6] \"……시미코?\"",
      "VoiceId": 0
    },
    {
      "TextJp": "……いえ、お気になさらず。さあ、早速探すとしましょうか！",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2600406,
      "SelectionGroup": 5,
      "BGMId": 0,
      "Sound": "",

      "PopupFileName": "",
      "ScriptKr": "5;마키;01\n#5;h\n#5;greeting\n#wait;2500",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1000706,
      "SelectionGroup": 0,

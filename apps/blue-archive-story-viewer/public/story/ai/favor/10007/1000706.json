      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s] “알았어.”",
      "VoiceId": 0
    },
    {
      "TextJp": "嘻嘻，谢谢你。",
      "TextCn": "",

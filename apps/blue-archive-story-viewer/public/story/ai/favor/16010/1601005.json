    {
      "TextJp": "[ns3]  \"……不太清楚。\"\n[ns4]  \"可能不太想工作呢……。\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1601005,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns3] \"……잘 모르겠어.\"\n[ns4] \"여전히 일하고 싶지는 않은데…….\"",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1601005,
      "SelectionGroup": 3,
      "BGMId": 0,

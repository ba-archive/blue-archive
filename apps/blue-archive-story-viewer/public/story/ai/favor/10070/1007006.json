      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#place;매화원 내부",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1007006,
      "SelectionGroup": 0,

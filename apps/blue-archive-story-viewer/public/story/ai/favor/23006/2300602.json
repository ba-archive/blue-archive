      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s11] “으응, 뭐.”\n[s12] “…….”",
      "VoiceId": 0

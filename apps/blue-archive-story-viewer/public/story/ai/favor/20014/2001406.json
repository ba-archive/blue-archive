      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;사키 헬멧;10;장난치지 말고 빨리!\n#3;hophop",
      "VoiceId": 0
    },
    {
      "TextJp": "在犹豫什么，白痴！",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2001406,
      "SelectionGroup": 4,

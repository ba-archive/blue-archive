      "VoiceId": 0
    },
    {
      "TextJp": "[ns] “等一下！”",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2000402,

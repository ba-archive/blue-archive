      "VoiceId": 0
    },
    {
      "TextJp": "[s]  \"艾米？\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2003205,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s] \"에이미?\"",
      "VoiceId": 0
    },
    {
      "TextJp": "啊，老师？",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2003205,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "SFX_Emoticon_Motion_Question",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;에이미 수영복ND;어? 선생님?",
      "VoiceId": 0
    },
    {
      "TextJp": "等着，马上就来。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",

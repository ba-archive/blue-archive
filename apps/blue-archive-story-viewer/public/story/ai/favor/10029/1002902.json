    {
      "TextJp": "[ns3]  “手工饼干？”\n[ns4]  “看起来好好吃。”",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002902,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns3] \"수제 쿠키구나.\"\n[ns4] \"맛있어 보이네.\"",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",

    {
      "TextJp": "[ns]  “若藻……！？”",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1003306,
      "SelectionGroup": 0,
      "BGMId": 0,

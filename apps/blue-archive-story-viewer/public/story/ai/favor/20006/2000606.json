      "ScriptKr": "#Title;유행의 법칙",
      "VoiceId": 0
    },
    {
      "TextJp": "山海经・咖啡店“桃源郷”",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2000606,
      "SelectionGroup": 0,
      "BGMId": 0,

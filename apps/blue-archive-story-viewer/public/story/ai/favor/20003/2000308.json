      "VoiceId": 0
    },
    {
      "TextJp": "在安慰着沮丧的真白后，回到了学园。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2000308,
      "SelectionGroup": 0,
      "BGMId": 0,

      "ScriptKr": "#wait;4000",
      "VoiceId": 0
    },
    {
      "TextJp": "― 还是您应该回去工作了吧……。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001109,
      "SelectionGroup": 0,

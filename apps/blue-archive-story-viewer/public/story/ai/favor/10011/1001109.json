    {
      "TextJp": "梅花园・休息室",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001109,
      "SelectionGroup": 0,
      "BGMId": 0,

    {
      "TextJp": "三一圣堂后面的公园",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1003605,
      "SelectionGroup": 0,

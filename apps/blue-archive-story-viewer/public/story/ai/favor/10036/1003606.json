      "VoiceId": 0
    },
    {
      "TextJp": "沿着河岸",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1003606,
      "SelectionGroup": 0,
      "BGMId": 0,

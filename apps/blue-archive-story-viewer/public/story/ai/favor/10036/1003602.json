    {
      "TextJp": "呼……结束了。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1003602,
      "SelectionGroup": 0,
      "BGMId": 0,

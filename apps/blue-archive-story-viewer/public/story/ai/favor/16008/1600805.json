      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "5;후부키;99\n#5;em;…\n#5;a\n#wait;2500",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1600805,

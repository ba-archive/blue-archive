      "PopupFileName": "",
      "ScriptKr": "1;카요코;02\n#1;ar\n#1;h\n#wait;1000",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300502,
      "SelectionGroup": 0,

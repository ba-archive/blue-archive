      "BGName": 1189606028,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;카요코;03",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300508,

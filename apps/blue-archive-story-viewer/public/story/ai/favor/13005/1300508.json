    {
      "TextJp": "商店街·唱片店前",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300508,
      "SelectionGroup": 0,
      "BGMId": 0,

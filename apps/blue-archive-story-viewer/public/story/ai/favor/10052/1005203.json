      "VoiceId": 0
    },
    {
      "TextJp": "……簡単にはいきませんよ？#nふふ。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1005203,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;노아;03;……아마 쉽지 않을 거예요. 후후.\n#3;em;[음표]\n#3;closeup",
      "VoiceId": 0

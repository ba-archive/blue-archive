      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s] \"그건 어렵지 않지만…….\"",
      "VoiceId": 0
    },

    {
      "TextJp": "……。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1005203,
      "SelectionGroup": 0,
      "BGMId": 0,

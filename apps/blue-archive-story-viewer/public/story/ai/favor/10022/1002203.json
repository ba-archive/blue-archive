      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002203,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;히나 수영복ND;10;자, 선생……. 저녁, 어떻게 할래?",
      "VoiceId": 0
    },
    {

    },
    {
      "TextJp": "啊……。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300809,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;세리카;11;아…….",
      "VoiceId": 0
    },
    {
      "TextJp": "为了夜间巡逻的兼职……。",
      "TextCn": "",
      "TextKr": "",

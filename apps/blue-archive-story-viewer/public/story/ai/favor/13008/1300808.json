  "content": [
    {
      "TextJp": "意外的幸运",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300808,
      "SelectionGroup": 0,

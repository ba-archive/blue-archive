    {
      "TextJp": "[ns1]  \"……怎么了？\"\n[ns2]  \"……为什么要去卡拉OK？\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1005106,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns1] \"……그래서?\"\n[ns2] \"……어째서 노래방인 거지?\"",
      "VoiceId": 0
    },
    {
      "TextJp": "",

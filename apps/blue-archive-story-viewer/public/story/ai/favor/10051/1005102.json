      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns] \"우타하의 냄새가 나.\"",
      "VoiceId": 0
    },
    {
      "TextJp": "……额，我的味道？",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1005102,

    {
      "TextJp": "格黑娜附近·温泉乡",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1003002,
      "SelectionGroup": 0,
      "BGMId": 0,

      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;미아의 언니;네……? 니, 닌자……?",
      "VoiceId": 0
    },
    {
      "TextJp": "高个子姐姐，谢谢你！",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1004003,
      "SelectionGroup": 0,
      "BGMId": 0,

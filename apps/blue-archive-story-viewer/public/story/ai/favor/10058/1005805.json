      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns] \"싫어어어어어어!\"",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",

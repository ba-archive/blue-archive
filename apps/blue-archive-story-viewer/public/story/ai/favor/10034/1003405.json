    {
      "TextJp": "花嫁修行其二：打扫篇：",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1003405,
      "SelectionGroup": 0,
      "BGMId": 10,

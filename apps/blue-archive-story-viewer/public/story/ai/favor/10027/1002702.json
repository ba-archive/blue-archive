    {
      "TextJp": "[ns]  \"……？\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002702,
      "SelectionGroup": 0,
      "BGMId": 0,

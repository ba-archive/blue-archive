      "ScriptKr": "3;카린;02;……아.",
      "VoiceId": 0
    },
    {
      "TextJp": "……？",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002705,
      "SelectionGroup": 0,

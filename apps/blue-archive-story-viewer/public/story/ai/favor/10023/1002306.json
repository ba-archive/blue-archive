      "PopupFileName": "",
      "ScriptKr": "3;이오리 수영복ND;01\n#3;d\n#wait;1000",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002306,

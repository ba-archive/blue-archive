      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "1;수영복 불량배 MG;05;쳇, 벌써 신고가 들어간 건가!\n#1;stiff",
      "VoiceId": 0
    },
    {
      "TextJp": "正面攻击无效，我们逃！",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002305,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",

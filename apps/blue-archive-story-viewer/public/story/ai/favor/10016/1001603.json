      "BGName": 1189606028,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;미도리;07\n#3;h",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001603,
      "SelectionGroup": 0,
      "BGMId": 0,

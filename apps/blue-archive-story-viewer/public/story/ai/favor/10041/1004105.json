      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1004105,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s] \"마시기 쉽게, 단 걸로 했어.\"",
      "VoiceId": 0
    },
    {
      "TextJp": "……。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",

      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;미사키;19;…….\n#3;stiff",
      "VoiceId": 0
    },
    {

      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002102,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s] \"죽지 않을 병도 그러다 죽겠다!\"",

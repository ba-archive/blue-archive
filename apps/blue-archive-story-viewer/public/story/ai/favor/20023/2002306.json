    },
    {
      "TextJp": "……这样真的对吗？",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002306,
      "SelectionGroup": 0,
      "BGMId": 0,

      "VoiceId": 0
    },
    {
      "TextJp": "[s] \"海呀ーーーーー！！！！\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2000503,
      "SelectionGroup": 0,

      "VoiceId": 0
    },
    {
      "TextJp": "呜呜，姐姐～你在哪里～",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2000520,
      "SelectionGroup": 0,
      "BGMId": 0,

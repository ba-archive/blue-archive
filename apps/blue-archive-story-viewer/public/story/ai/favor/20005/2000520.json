      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;미아가 된 꼬마 아가씨 2;02;으앙, 언니~ 어디 간 거야~\n#3;stiff",
      "VoiceId": 0
    },
    {
      "TextJp": "这里哪里～姐姐～",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2000520,
      "SelectionGroup": 0,
      "BGMId": 0,

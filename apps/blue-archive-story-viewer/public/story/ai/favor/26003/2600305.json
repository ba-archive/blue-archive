      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;세리나;05\n#3;greeting\n#wait;450",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",

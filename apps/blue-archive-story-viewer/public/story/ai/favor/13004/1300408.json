      "ScriptKr": "#Title;건강해지는 거예요",
      "VoiceId": 0
    },
    {
      "TextJp": "阿拜多斯高级中学·操场",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300408,

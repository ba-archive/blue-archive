      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "4;노노미;12;비, 비밀이에요.",
      "VoiceId": 0
    },
    {
      "TextJp": "[s1]  \"嗯，我有点在意呢……\"\n[s2]  \"肯定是有什么情况吧。\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300415,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,

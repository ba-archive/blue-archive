      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "4;노노미;14;아, 그게에……\n#4;stiff",
      "VoiceId": 0
    },
    {
      "TextJp": "呃，是秘密。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300415,
      "SelectionGroup": 0,
      "BGMId": 0,

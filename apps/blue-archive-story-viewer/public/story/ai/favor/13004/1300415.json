      "VoiceId": 0
    },
    {
      "TextJp": "那个，那个……",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300415,
      "SelectionGroup": 0,
      "BGMId": 0,

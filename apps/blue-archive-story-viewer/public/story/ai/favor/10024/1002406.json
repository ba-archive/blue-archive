      "VoiceId": 0
    },
    {
      "TextJp": "这次能做到这些真是太好了。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002406,
      "SelectionGroup": 0,

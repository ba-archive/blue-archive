      "VoiceId": 0
    },
    {
      "TextJp": "哈呜。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2000206,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;사야;06;하읏.\n#3;shake\n#3;a\n#3;em;[?!]",
      "VoiceId": 0
    },

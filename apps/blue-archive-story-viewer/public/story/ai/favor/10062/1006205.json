      "ScriptKr": "#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "商店街",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006205,
      "SelectionGroup": 0,

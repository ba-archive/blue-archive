      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;(잠을 잘까 했지만, 커피를 많이 마신 탓인지 잠이 오지 않는다.)",
      "VoiceId": 0
    },
    {
      "TextJp": "（经过犹豫，决定出去透口气）",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300006,

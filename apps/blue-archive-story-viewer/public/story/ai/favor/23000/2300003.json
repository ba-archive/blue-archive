      "VoiceId": 0
    },
    {
      "TextJp": "千层蛋糕咖啡厅",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300003,
      "SelectionGroup": 0,

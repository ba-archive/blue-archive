      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1600005,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;점원;브레이킹 뉴스? ……갑자기 속보인가?",
      "VoiceId": 0

      "GroupId": 1600002,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;하루카;10;포, 폭탄……\n#3;em;…",
      "VoiceId": 0
    },
    {
      "TextJp": "设置了炸弹……。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1600002,

  "content": [
    {
      "TextJp": "パンがなければ：",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002406,
      "SelectionGroup": 0,
      "BGMId": 4,

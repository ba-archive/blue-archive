    {
      "TextJp": "呜嘻……老师……",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006806,
      "SelectionGroup": 0,
      "BGMId": 0,

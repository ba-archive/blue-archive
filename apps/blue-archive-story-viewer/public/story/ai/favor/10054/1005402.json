      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "2;마리 체육복ND;02;……네?\n#2;em;[?]",
      "VoiceId": 0
    },

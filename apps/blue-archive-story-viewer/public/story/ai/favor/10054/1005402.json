      "VoiceId": 0
    },
    {
      "TextJp": "真是太好了，是老师呢。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1005402,
      "SelectionGroup": 0,

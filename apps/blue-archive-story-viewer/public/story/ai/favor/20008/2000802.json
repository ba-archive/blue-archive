      "TextTh": "",
      "TextTw": "",
      "GroupId": 2000802,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s] \"아니, 그건 전혀 신경 안쓰는데, 그…….\"",
      "VoiceId": 0
    },

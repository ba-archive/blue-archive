  "content": [
    {
      "TextJp": "準備是万全的",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1600206,
      "SelectionGroup": 0,
      "BGMId": 999,

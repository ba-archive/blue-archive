      "ScriptKr": "#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "好好吃啊！",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1601203,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;준코 새해;03;맛있었어!\n#3;em;[음표]\n#3;closeup\n#3;greeting\n#3;a",
      "VoiceId": 0
    },
    {
      "TextJp": "[s]  \"吃了好多啊。肚子没事吧？\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1601203,
      "SelectionGroup": 0,
      "BGMId": 0,

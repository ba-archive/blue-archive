      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#title;새해에는 새 옷으로!",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1601202,

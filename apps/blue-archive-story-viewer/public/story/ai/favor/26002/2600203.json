      "VoiceId": 0
    },
    {
      "TextJp": "格黑娜・第8学生食堂",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2600203,
      "SelectionGroup": 0,
      "BGMId": 0,

    {
      "TextJp": "（……再次遇见小玉）",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1008403,
      "SelectionGroup": 0,
      "BGMId": 0,

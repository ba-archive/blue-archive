    {
      "TextJp": "哎呀，哎呀……",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1008406,
      "SelectionGroup": 0,
      "BGMId": 0,

      "ScriptKr": "3;츠바키;01\n#3;greeting\n#wait;400",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1300903,
      "SelectionGroup": 0,

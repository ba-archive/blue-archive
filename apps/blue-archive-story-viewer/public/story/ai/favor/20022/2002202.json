      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002202,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "4;후우카;03;뜨거운 상태에서 먹어야 맛있대요!",
      "VoiceId": 0

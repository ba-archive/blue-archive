      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "4;후우카;07;후우…… 오늘도 힘들었네……\n#4;stiff",
      "VoiceId": 0
    },
    {
      "TextJp": "……老师！？",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",

    {
      "TextJp": "你好，老师。\r",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002203,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;후우카 새해ND;99;어서 오세요, 선생님.\n#3;greeting",
      "VoiceId": 0
    },
    {
      "TextJp": "[ns]  \"哎呀，枫香。这身衣服……？\"\r",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002203,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns] \"어라, 후우카. 옷이……?\"",
      "VoiceId": 0
    },

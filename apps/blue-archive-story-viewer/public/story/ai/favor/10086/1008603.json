    {
      "TextJp": "这个……老师？",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1008603,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "1;히나 드레스;08;어, 어떨까…… 선생님?\n#1;em;[///]\n#1;stiff",
      "VoiceId": 0
    },
    {
      "TextJp": "适合吗，我……？",

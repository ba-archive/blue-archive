      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "1;치히로;03\n#1;m4\n#wait;1000",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2001305,
      "SelectionGroup": 0,

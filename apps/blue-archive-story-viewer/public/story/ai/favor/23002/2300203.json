  "content": [
    {
      "TextJp": "不眠症",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300203,
      "SelectionGroup": 0,
      "BGMId": 6,
      "Sound": "",

    },
    {
      "TextJp": "……！！センセ……いえ、お頭！！",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1600402,
      "SelectionGroup": 0,
      "BGMId": 0,

      "BGName": 1047754314,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "握住我的手。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1000808,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;네루;내 손을 잡아.",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1000808,
      "SelectionGroup": 0,

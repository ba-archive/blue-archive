      "PopupFileName": "",
      "ScriptKr": "3;네루;01\n#3;greeting\n#wait;1000\n#3;black",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1000805,
      "SelectionGroup": 0,
      "BGMId": 0,

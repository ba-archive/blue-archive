    {
      "TextJp": "― 在基沃托斯，魔法是不可思议的。[wa:2500]",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001508,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 3006928443,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#st;[-1200,-530];serial;60;― 아리스는 키보토스에, [wa:2500]마법이 없다고 생각하지 않습니다.",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001508,

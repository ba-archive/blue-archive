    },
    {
      "TextJp": "在住宅区狭窄的小巷里。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1000608,
      "SelectionGroup": 0,
      "BGMId": 0,

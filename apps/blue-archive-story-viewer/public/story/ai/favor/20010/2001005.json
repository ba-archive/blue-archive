      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#Place;구교사 227호 인근의 산길",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2001005,
      "SelectionGroup": 0,

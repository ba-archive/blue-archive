      "ScriptKr": "#Title;게으름뱅이의 아침",
      "VoiceId": 0
    },
    {
      "TextJp": "对策委员会·教室",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1000503,

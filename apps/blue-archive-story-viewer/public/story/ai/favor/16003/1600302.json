      "PopupFileName": "",
      "ScriptKr": "#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "……！？",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1600302,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",

      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;마리;04;……?\n#3;em;[?]\n#3;stiff",
      "VoiceId": 0
    },
    {
      "TextJp": "……原来如此。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300805,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",

      "VoiceId": 0
    },
    {
      "TextJp": "好了，这样应该没问题了。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006902,
      "SelectionGroup": 0,
      "BGMId": 0,

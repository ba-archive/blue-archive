      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006902,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",

      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001206,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 1122508889,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "（之后拿到点的甜品，坐到了咖啡厅里）",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",

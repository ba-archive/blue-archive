    },
    {
      "TextJp": "ミレニアム・ジム",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001205,
      "SelectionGroup": 0,

      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001205,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;스미레;03;아하! 빨리 트레이닝을 하고 싶으신거죠?\n#3;jump\n#3;em;[반응]",
      "VoiceId": 0
    },

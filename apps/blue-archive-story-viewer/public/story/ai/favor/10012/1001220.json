    {
      "TextJp": "哈、哈……。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001220,
      "SelectionGroup": 0,
      "BGMId": 0,

    {
      "TextJp": "游戏开发部·部室",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001802,
      "SelectionGroup": 0,
      "BGMId": 0,

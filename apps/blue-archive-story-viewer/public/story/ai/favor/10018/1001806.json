      "VoiceId": 0
    },
    {
      "TextJp": "游乐园·正门入口：",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001806,
      "SelectionGroup": 0,

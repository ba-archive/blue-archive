      "TextTw": "",
      "GroupId": 2000102,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;카린;04;그냥 같이 잡혀 왔다고?\n#3;stiff",
      "VoiceId": 0
    },
    {

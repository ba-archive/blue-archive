      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2000903,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",

      "VoiceId": 0
    },
    {
      "TextJp": "[ns3]  \"这，原来如此……\"\n[ns4]  \"终于这一天到来了啊。\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006108,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns3] \"아.아.아. 그렇군요. 네.\"\n[ns4] \"결국 이날이 찾아왔구나.\"",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006108,
      "SelectionGroup": 3,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#st;[0,0];instant;1;",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",

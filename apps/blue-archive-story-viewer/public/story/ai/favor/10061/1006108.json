    {
      "TextJp": "[ns5]  \"诶……！？\"\n[ns6]  \"那样反而会加深怀疑……！\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006108,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns5] \"……예? 뭘 하셨다고요?\"\n[ns6] \"도리어 의혹이 커져만 가는 일을…….\"",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006108,
      "SelectionGroup": 5,

    },
    {
      "TextJp": "[ns]  \"等等，你要做什么？\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006108,
      "SelectionGroup": 0,
      "BGMId": 0,

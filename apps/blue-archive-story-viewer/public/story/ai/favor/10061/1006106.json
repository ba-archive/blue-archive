    {
      "TextJp": "いらっしゃいましたか、先生。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1006106,
      "SelectionGroup": 0,

      "ScriptKr": "2;노도카;06\n#2;a\n#2;em;[...]\n#wait;2000",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2600605,
      "SelectionGroup": 0,

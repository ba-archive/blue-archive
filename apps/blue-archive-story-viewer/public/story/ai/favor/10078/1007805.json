  "content": [
    {
      "TextJp": "知性之姿：",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1007805,
      "SelectionGroup": 0,
      "BGMId": 999,
      "Sound": "",

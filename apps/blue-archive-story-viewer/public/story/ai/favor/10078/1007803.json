    },
    {
      "TextJp": "哦～，真不错，老师！时间刚刚好！",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1007803,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "SFX_Emoticon_Motion_Music",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;카스미;오~ 역시 선생님! 시간 약속 한번 철저하네!",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",

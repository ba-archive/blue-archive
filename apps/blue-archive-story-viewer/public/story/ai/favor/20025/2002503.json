      "VoiceId": 0
    },
    {
      "TextJp": "[ns1] “遥香！”\n[ns2] “这边！”",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002503,
      "SelectionGroup": 0,
      "BGMId": 0,

      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns3] \"……잠깐만, 뭐라고?\"\n[ns4] \"……예!?\"",
      "VoiceJp": ""
    },
    {
      "TextJp": "",
      "TextCn": "",

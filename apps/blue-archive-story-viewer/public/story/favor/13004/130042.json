    {
      "TextJp": "（ビクッ）",
      "TextCn": "（惊）",
      "TextKr": "",
      "TextEn": "*surprised*",
      "TextTh": "(ตกใจ)",
      "TextTw": "（大吃一驚）",
      "GroupId": 1300402,

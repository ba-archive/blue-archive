{
  "GroupId": 1004902,
  "translator": "今天早睡了吗",
  "content": [
    {
      "TextJp": "補習授業とスイーツ",
      "TextCn": "补习与甜品",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1004902,
      "SelectionGroup": 0,
      "BGMId": 999,

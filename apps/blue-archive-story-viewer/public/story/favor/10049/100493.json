    },
    {
      "TextJp": "……あ。",
      "TextCn": "……啊。",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1004903,
      "SelectionGroup": 0,
      "BGMId": 0,

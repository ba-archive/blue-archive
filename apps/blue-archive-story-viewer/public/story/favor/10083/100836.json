      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1008306,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns] \"그러는 오늘의 키쿄는 좀 신선한 모습인걸.\"",
      "VoiceJp": ""
    },

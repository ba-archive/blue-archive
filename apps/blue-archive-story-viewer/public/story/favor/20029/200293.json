      "PopupFileName": "",
      "ScriptKr": "3;미모리 수영복ND;06\n#3;d\n#wait;1000",
      "VoiceJp": ""
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002903,

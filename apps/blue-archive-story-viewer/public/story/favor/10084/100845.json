      "VoiceId": 0
    },
    {
      "TextJp": "（しばらくして……）",
      "TextCn": "（过了一会儿……）",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1008405,

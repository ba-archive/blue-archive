      "ScriptKr": "#zmc;instant;0,-222;2528\n#wait;1000",
      "VoiceId": 0
    },
    {
      "TextJp": "先生、いらっしゃいますか？",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300002,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#zmc;instant;0,-222;2528\n#na;???;선생님, 계신가요?",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300002,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "SE_Gear_02",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#zmc;move;0,-222;2528;10",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300002,

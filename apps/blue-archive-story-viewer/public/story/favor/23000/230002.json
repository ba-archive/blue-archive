      "ScriptKr": "#zmc;instant;0,-222;2528\n#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300002,
      "SelectionGroup": 0,
      "BGMId": 0,

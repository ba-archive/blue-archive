      "VoiceId": 0
    },
    {
      "TextJp": "……いらっしゃいましたか、先生。",
      "TextCn": "……您来了啊，老师。",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010603,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "SFX_Emoticon_Motion_Respond",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;???;……아. 오셨군요, 선생님.",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",

      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010603,
      "SelectionGroup": 0,

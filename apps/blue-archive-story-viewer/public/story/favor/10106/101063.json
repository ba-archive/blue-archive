      "ScriptKr": "3;사쿠라코;06\n#3;em;…\n#wait;2500",
      "VoiceId": 0
    },
    {
      "TextJp": "……先生。",
      "TextCn": "……老师。",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010603,

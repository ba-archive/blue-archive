{
  "GroupId": 1010605,
  "translator": "Sino",
  "proofreader": "今天早睡了吗",
  "content": [
    {
      "TextJp": "アイドルの輝き",
      "TextCn": "偶像的光芒",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010605,
      "SelectionGroup": 0,
      "BGMId": 240,
      "Sound": "",

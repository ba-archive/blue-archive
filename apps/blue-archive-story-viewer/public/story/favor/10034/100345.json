      "VoiceId": 0
    },
    {
      "TextJp": "（ごしごしごし……）",
      "TextCn": "（咯吱咯吱咯吱……）",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1003405,

      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;미모리;00;미술용 붓이랍니다!\n#3;closeup\n#3;em;[!]",
      "VoiceId": 0
    },
    {
      "TextJp": "[s] \"意外なアイテムが！\"",
      "TextCn": "[s]“出乎意料的道具！”",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1003405,
      "SelectionGroup": 0,
      "BGMId": 0,

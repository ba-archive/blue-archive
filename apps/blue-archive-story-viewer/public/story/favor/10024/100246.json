      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#clearST",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002406,
      "SelectionGroup": 0,
      "BGMId": 0,

      "PopupFileName": "",
      "ScriptKr": "3;시로코 라이딩;01\n#3;d\n#wait;1000",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002406,
      "SelectionGroup": 0,

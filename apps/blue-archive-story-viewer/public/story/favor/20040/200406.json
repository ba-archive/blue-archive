      "Sound": "SFX_Emoticon_Motion_Dot",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;사츠키;…… ",
      "VoiceId": 0
    },

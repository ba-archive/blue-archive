      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "2;시로코;01\n#wait;1500\n#2;a",
      "VoiceJp": ""
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001006,
      "SelectionGroup": 0,

      "VoiceJp": ""
    },
    {
      "TextJp": "……うーん。",
      "TextCn": "……嗯。",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001003,

    {
      "TextJp": "[s] \"……シロコ？\"",
      "TextCn": "[s] “……白子？”",
      "TextKr": "",
      "TextEn": "[s] \"Shiroko?\"",
      "TextTh": "[s] \"ชิโรโกะ...?\"",
      "TextTw": "[s] 「……白子？」",

      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;이부키;99;음…….\n#3;em;…",
      "VoiceId": 0
    },
    {
      "TextJp": "まだよく分からなかった！",
      "TextCn": "伊吹还是不太明白！",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1601405,
      "SelectionGroup": 0,
      "BGMId": 50,
      "Sound": "",

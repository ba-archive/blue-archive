    {
      "TextJp": "ふええぇぇえん……。",
      "TextCn": "呜诶诶诶……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1301303,
      "SelectionGroup": 0,
      "BGMId": 130,

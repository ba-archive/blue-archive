    {
      "TextJp": "ミレニアム・外郭地区",
      "TextCn": "千年・郊区",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1001503,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",

      "VoiceId": 0
    },
    {
      "TextJp": "ふむ……なるほど……その本を選ぶとは……",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1011005,

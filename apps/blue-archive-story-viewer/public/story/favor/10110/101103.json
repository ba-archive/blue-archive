      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;세이아;04\n#3;em;…\n#wait;2500",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1011003,
      "SelectionGroup": 0,

    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1007206,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",

      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;노아 파자마;04;역시……\n#3;em;…",
      "VoiceId": 0
    },
    {
      "TextJp": "[s] \"うん……。\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010903,

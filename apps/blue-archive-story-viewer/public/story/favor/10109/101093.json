      "ScriptKr": "#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "（その後も模様替えを繰り返した）",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010903,
      "SelectionGroup": 0,
      "BGMId": 0,

    {
      "TextJp": "[ns1] \"はい、お待たせ。\"\n[ns2] \"熱いから気をつけ――\"",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010906,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns1] \"자, 여기 블랙커피.\"\n[ns2] \"가득 담았으니 마실 때 조심히—\"",
      "VoiceId": 0
    },

      "VoiceId": 0
    },
    {
      "TextJp": "……。",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010906,
      "SelectionGroup": 0,

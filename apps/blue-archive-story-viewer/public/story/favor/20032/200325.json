      "ScriptKr": "#na;(생각보다 일이 빨리 마무리되어서 특무부실로 향했다.)",
      "VoiceJp": "",
      "Unsure": false
    },
    {
      "TextJp": "[ns] \"エイミ、いる？\"",
      "TextCn": "[ns]“艾米，在吗？”",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2003205,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns] \"에이미. 나 왔어.\"",
      "VoiceJp": ""
    },
    {
      "TextJp": "うーん……",
      "TextCn": "唔——嗯……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2003205,
      "SelectionGroup": 0,

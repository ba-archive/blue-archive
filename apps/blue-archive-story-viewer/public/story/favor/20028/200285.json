    {
      "TextJp": "……！！",
      "TextCn": "……！！",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002805,
      "SelectionGroup": 0,
      "BGMId": 0,

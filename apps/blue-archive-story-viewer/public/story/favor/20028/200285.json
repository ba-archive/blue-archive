      "ScriptKr": "4;히나타 수영복;09\n#4;em;…\n#wait;2500",
      "VoiceJp": ""
    },
    {
      "TextJp": "こ……",
      "TextCn": "我……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002805,
      "SelectionGroup": 0,
      "BGMId": 0,

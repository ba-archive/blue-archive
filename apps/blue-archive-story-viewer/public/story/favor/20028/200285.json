      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002805,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;히나타 수영복;12;네. 선생님. 말씀해주세요.\n#3;closeup",
      "VoiceJp": ""
    },

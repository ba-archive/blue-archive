      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "（……待った）",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2600303,
      "SelectionGroup": 0,

      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#all;hide",
      "VoiceId": 0
    },
    {
      "TextJp": "（まるで魔法のように、本当にまぶたが重くなっていく）",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",

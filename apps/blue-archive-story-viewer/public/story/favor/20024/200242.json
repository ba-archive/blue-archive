      "Sound": "SE_Cartoon_02",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#wait;1500",
      "VoiceId": 0
    },
    {
      "TextJp": "……。",
      "TextCn": "……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002402,
      "SelectionGroup": 0,
      "BGMId": 999,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;나기사;99;흐음…….\n#3;em;…",
      "VoiceId": 0
    },
    {

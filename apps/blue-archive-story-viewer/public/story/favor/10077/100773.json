    {
      "TextJp": "……ん？",
      "TextCn": "……嗯？",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1007703,
      "SelectionGroup": 0,
      "BGMId": 0,

    {
      "TextJp": "レッドウィンター連邦学園・図書館",
      "TextCn": "红冬联邦学园・图书馆",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2002603,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",

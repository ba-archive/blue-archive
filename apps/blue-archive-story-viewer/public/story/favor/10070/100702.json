    {
      "TextJp": "[ns] \"……ん？\"",
      "TextCn": "[ns] “……嗯？”",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1007002,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns] \"응? 전화벨 소리의 위치가……?\"",
      "VoiceJp": ""
    },
    {
      "TextJp": "（着信音がドアの向こうから聞こえてきた）",
      "TextCn": "（门后传来了手机铃声）",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1007002,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "SE_Ring_02",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;(돌연 전화벨 소리가 집무실 문 밖에서 들려 오기 시작했다!)",
      "VoiceJp": ""
    },
    {
      "TextJp": "おわあっ！",
      "TextCn": "哦哇啊！",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",

    },
    {
      "TextJp": "そうだよね……",
      "TextCn": "说得也是……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1005903,
      "SelectionGroup": 0,
      "BGMId": 0,

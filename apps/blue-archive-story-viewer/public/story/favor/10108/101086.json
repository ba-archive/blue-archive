    {
      "TextJp": "片付けも終わったし……。",
      "TextCn": "也收拾完了……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010806,
      "SelectionGroup": 0,
      "BGMId": 0,

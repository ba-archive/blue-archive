    {
      "TextJp": "……先生？",
      "TextCn": "……老师？",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1008003,
      "SelectionGroup": 0,
      "BGMId": 0,

    },
    {
      "TextJp": "あ、先生……。",
      "TextCn": "啊，老师……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2601006,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "SFX_Emoticon_Motion_Dot",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;???;아, 선생님…….",
      "VoiceJp": ""
    },
    {
      "TextJp": "こ、こんにちは……。",
      "TextCn": "早、早上好……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2601006,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;???;이, 이쪽이에요……",
      "VoiceJp": ""
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2601006,

      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;미유 수영복;03",
      "VoiceJp": ""
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2601006,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "SE_Close_01",

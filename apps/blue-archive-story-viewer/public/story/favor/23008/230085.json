    },
    {
      "TextJp": "[ns5] \"……マリー？\"\n[ns6] \"どうしたの？\"",
      "TextCn": "[ns5] “……玛丽？”\n[ns6] “怎么了？”",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300805,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[ns5] \"……마리?\"\n[ns6] \"뭔가 할 말이라도 있어?\"",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300805,
      "SelectionGroup": 5,
      "BGMId": 0,

{
  "GroupId": 2300806,
  "translator": "今天早睡了吗",
  "content": [
    {
      "TextJp": "私の望み",
      "TextCn": "我的心愿",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300806,
      "SelectionGroup": 0,
      "BGMId": 999,

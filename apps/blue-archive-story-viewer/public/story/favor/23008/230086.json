      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;마리;02;……선…….",
      "VoiceId": 0
    },
    {
      "TextJp": "[s5] \"……に？\"\n[s6] \"……？\"",
      "TextCn": "[s5] “……起？”\n[s6] “……？”",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300806,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",
      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "[s5] \"……선?\"\n[s6] \"……마리?\"",
      "VoiceId": 0
    },
    {
      "TextJp": "先生と、一緒に……。",
      "TextCn": "和老师，一起……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 2300806,
      "SelectionGroup": 5,
      "BGMId": 0,

      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "#na;(이후 치아키와 함께 <주간 만마전>을 읽었다…….)",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1010702,
      "SelectionGroup": 0,
      "BGMId": 0,
      "Sound": "",

      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;나츠;02;……음. 그…….\n#3;em;[반응]\n#3;m3",
      "VoiceId": 0
    },
    {
      "TextJp": "うーん……",
      "TextCn": "唔嗯……",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002903,
      "SelectionGroup": 0,
      "BGMId": 0,

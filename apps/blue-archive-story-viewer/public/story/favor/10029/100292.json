      "ScriptKr": "3;나츠;01\n#3;m4\n#wait;1200",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002902,
      "SelectionGroup": 0,
      "BGMId": 0,

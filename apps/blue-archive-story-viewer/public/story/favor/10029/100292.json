      "ScriptKr": "[ns3] \"수제 쿠키구나.\"\n[ns4] \"맛있어 보이네.\"",
      "VoiceId": 0
    },
    {
      "TextJp": "",
      "TextCn": "",
      "TextKr": "",
      "TextEn": "",
      "TextTh": "",
      "TextTw": "",
      "GroupId": 1002902,

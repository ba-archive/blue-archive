{
  "name": "ba-online-toolbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:with-player": "rush build --to-except ba-story-player & vite --host",
    "check": "cross-env ESLINT_USE_FLAT_CONFIG=false vue-tsc --noEmit; eslint -c .eslintrc.json ./src/ --ext .js,.ts,.vue",
    "build:json": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint -c .eslintrc.json ./public/ && node ./CICD/build_json.mjs",
    "build": "pnpm build:json && vite build",
    "build:github": "pnpm build",
    "up": "taze major -I",
    "preview": "vite preview --host"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "@vueuse/math": "^10.2.1",
    "axios": "^1.7.2",
    "ba-story-player": "workspace:*",
    "crypto-js": ">=4.2.0",
    "element-plus": "^2.7.3",
    "file-saver": "^2.0.5",
    "js-yaml": "^4.1.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.29",
    "vue-router": "~4.3.0",
    "mitt": "^3.0.0",
    "cross-env": "^7.0.3",
    "unocss": "^0.59.0-beta.1",
    "@arco-design/web-vue": "~2.55.2",
    "xxhashjs": "^0.2.2",
    "@types/xxhashjs": "^0.2.2",
    "uuid": "~9.0.0",
    "radash": "~12.1.0",
    "mockjs": "~1.1.0",
    "vite-plugin-mock": "~3.0.2",
    "diff": "~5.2.0",
    "diff2html": "~3.4.48"
  },
  "devDependencies": {
    "taze": "^0.13.8",
    "@csstools/css-parser-algorithms": "^2.6.3",
    "@csstools/css-tokenizer": "^2.3.1",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/crypto-js": "^4.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/js-yaml": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-legacy": "^5.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.27",
    "@vue/eslint-config-typescript": "^13.0.0",
    "chalk": "^5.3.0",
    "eslint": "9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-yaml": "^0.5.0",
    "naive-ui": "^2.34.4",
    "postcss": "~8.4.35",
    "postcss-plugin-px2rem": "^0.8.1",
    "postcss-preset-env": "^9.5.13",
    "prettier": "^2.8.8",
    "sass": "^1.77.2",
    "terser": "^5.31.0",
    "typescript": "^5.4.3",
    "unplugin-auto-import": "0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vfonts": "^0.0.3",
    "vite": "^5.2.11",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.19",

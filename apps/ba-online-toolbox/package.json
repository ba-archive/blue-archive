{
  "name": "ba-online-toolbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md",
    "check": "vue-tsc --noEmit; eslint --fix --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue",
    "build:json": "eslint --fix --config ./.eslintrc.json ./public/ && node ./CICD/build_json.mjs",
    "build": "pnpm format; pnpm build:json && vue-tsc && vite build",
    "preview": "vite preview --host"
  },
  "dependencies": {
    "@vueuse/core": "^10.0.0",
    "@vueuse/math": "^10.0.0",
    "axios": "1.4.0",
    "ba-story-player": "workspace:*",
    "crypto-js": "4.1.1",
    "element-plus": "^2.3.4",
    "file-saver": "2.0.5",
    "js-yaml": "4.1.0",
    "pinia": "2.1.3",
    "pinia-plugin-persistedstate": "3.1.0",
    "vue": "3.3.4",
    "vue-router": "4.2.1"
  },
  "devDependencies": {
    "@csstools/css-parser-algorithms": "^2.1.0",
    "@csstools/css-tokenizer": "^2.1.0",
    "@rushstack/eslint-patch": "1.3.2",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/crypto-js": "4.1.1",
    "@types/file-saver": "2.0.5",
    "@types/js-yaml": "4.0.5",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "@vitejs/plugin-legacy": "4.0.4",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/compiler-sfc": "3.x",
    "@vue/eslint-config-typescript": "11.0.3",
    "chalk": "5.2.0",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.14.1",
    "eslint-plugin-yaml": "0.5.0",
    "naive-ui": "2.34.4",
    "postcss": "8.4.24",
    "postcss-plugin-px2rem": "0.8.1",
    "postcss-preset-env": "8.4.1",
    "prettier": "2.8.8",
    "prettier-plugin-css-order": "1.3.1",
    "sass": "1.62.1",
    "terser": "5.17.6",
    "typescript": "5.0.4",
    "unplugin-auto-import": "^0.16.0",
    "unplugin-vue-components": "^0.25.0",
    "vfonts": "0.0.3",
    "vite": "4.3.9",
    "vue-eslint-parser": "9.3.0",
    "vue-tsc": "1.6.5"
  }
}
